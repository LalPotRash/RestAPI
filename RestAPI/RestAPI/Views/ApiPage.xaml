<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="RestAPI.Views.ApiPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    BackgroundColor="Black" >
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding API}" TextColor="Orange" />
            <Label Text="{Binding Description}" TextColor="Orange" />
            <Label Text="{Binding Auth}" TextColor="Orange" />
            <Label Text="{Binding HTTPS}" TextColor="Orange" />
            <Label Text="{Binding Cors}" TextColor="Orange" />
            <Label Text="{Binding Link}" TextColor="Orange" />
            <Label Text="{Binding Category}" TextColor="Orange" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>